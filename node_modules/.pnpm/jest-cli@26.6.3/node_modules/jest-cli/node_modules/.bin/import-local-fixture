#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/nxtwave/nodejs/coding-practices/coding-practice-1h/node_modules/.pnpm/import-local@3.1.0/node_modules/import-local/fixtures/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-1h/node_modules/.pnpm/import-local@3.1.0/node_modules/import-local/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-1h/node_modules/.pnpm/import-local@3.1.0/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-1h/node_modules/.pnpm/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-1h/node_modules:/home/nxtwave/nodejs/coding-practices/node_modules:/home/nxtwave/nodejs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-1h/node_modules/.pnpm/jest-cli@26.6.3/node_modules/jest-cli/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-1h/node_modules/.pnpm/jest-cli@26.6.3/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/nxtwave/nodejs/coding-practices/coding-practice-1h/node_modules/.pnpm/import-local@3.1.0/node_modules/import-local/fixtures/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-1h/node_modules/.pnpm/import-local@3.1.0/node_modules/import-local/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-1h/node_modules/.pnpm/import-local@3.1.0/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-1h/node_modules/.pnpm/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-1h/node_modules:/home/nxtwave/nodejs/coding-practices/node_modules:/home/nxtwave/nodejs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-1h/node_modules/.pnpm/jest-cli@26.6.3/node_modules/jest-cli/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-1h/node_modules/.pnpm/jest-cli@26.6.3/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../import-local@3.1.0/node_modules/import-local/fixtures/cli.js" "$@"
else
  exec node  "$basedir/../../../../../import-local@3.1.0/node_modules/import-local/fixtures/cli.js" "$@"
fi
